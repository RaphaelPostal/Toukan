<turbo-frame id="establishment-card">
        {% for section in card.sections %}
            <table>
                <tr>
                    {{ section.title }}
                </tr>
                <tr>
                    Ajouter un produit
                </tr>
                <tr>
                    <button type="button" onclick="set_frame_src({{ section.id }})" class="transition duration-150 ease-in-out admin-button-edit" data-bs-toggle="modal" data-bs-target="#modal-form-edit-section">
                        Modifier
                    </button>
                </tr>
                <tr>
                    Supprimer
                </tr>
                <tbody>
                {% for product in section.products %}
                    <tr>
                        <td>
                            {{ product.title }}
                        </td>
                        <td>
                            Modifier
                        </td>
                        <td>
                            Supprimer
                        </td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>
        {% endfor %}
</turbo-frame>
